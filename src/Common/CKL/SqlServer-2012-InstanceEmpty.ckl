		ON who.principal_id = what.grantee_principal_id
WHERE
	what.permission_name IN
	(
	'Administer bulk operations',
	'Alter any availability group',
	'Alter any connection',
	'Alter any credential',
	'Alter any database',
	'Alter any endpoint ',
	'Alter any event notification ',
